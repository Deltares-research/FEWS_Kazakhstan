	<gridPlotGroup id="Satellite precipitation" name="Satellite precipitation">
		<gridPlot id="Rain_gpm_grid_early" name="Precipitation GPM Early [mm]">
			<dataFeedId>GPMIMERG.Server.Precipitation</dataFeedId>
			<timeSeriesSet>
				<moduleInstanceId>Import_GPM_Server</moduleInstanceId>
				<valueType>grid</valueType>
				<parameterId>P.obs</parameterId>
				<qualifierId>Early</qualifierId>
				<locationId>IMERG</locationId>
				<timeSeriesType>external historical</timeSeriesType>
				<timeStep unit="minute" multiplier="30"/>
				<relativeViewPeriod unit="day" start="-4" end="0"/>
				<readWriteMode>read only</readWriteMode>
			</timeSeriesSet>
			<accumulationTimeSpan unit="hour" multiplier="1"/>
			<accumulationTimeSpan unit="hour" multiplier="3"/>
			<accumulationTimeSpan unit="hour" multiplier="6"/>
			<accumulationTimeSpan unit="hour" multiplier="12"/>
			<accumulationTimeSpan unit="day" multiplier="1"/>
			<accumulationTimeSpan unit="day" multiplier="2"/>
			<accumulationTimeSpan unit="day" multiplier="3"/>
			<accumulationTimeSpan unit="day" multiplier="7"/>
			<contourLinesColor>black</contourLinesColor>
		</gridPlot>
		<gridPlot id="Rain_gpm_catch" name="Precipitation GPM Catchments [mm]">
			<timeSeriesSet>
				<moduleInstanceId>Meteo_Process_Operational_Grids</moduleInstanceId>
				<valueType>scalar</valueType>
