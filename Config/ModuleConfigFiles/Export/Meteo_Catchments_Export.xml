<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesExportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesExportRun.xsd">
	<export>
		<general>
			<exportType>TSD</exportType>
			<folder>$REPORT_ROOT_DIR$\Forecasts</folder>
			<exportFileName>
				<name>GADM2_Catchments_GFS_Precipitation.</name>
				<suffix>
					<timeZeroFormattingString>yyyyMMddHHmm</timeZeroFormattingString>
				</suffix>
			</exportFileName>
			<exportMissingValue>-999</exportMissingValue>
			<exportTimeZone>
				<timeZoneOffset>+01:00</timeZoneOffset>
			</exportTimeZone>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>Meteo_Process_Operational_Grids</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.forecast</parameterId>
			<qualifierId>GFS</qualifierId>
			<locationSetId>GADM2</locationSetId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</export>
	<export>
		<general>
			<exportType>TSD</exportType>
			<folder>$REPORT_ROOT_DIR$\Forecasts</folder>
			<exportFileName>
				<name>GADM2_Catchments_GFS_Temperature.</name>
				<suffix>
					<timeZeroFormattingString>yyyyMMddHHmm</timeZeroFormattingString>
				</suffix>
			</exportFileName>
			<exportMissingValue>-999</exportMissingValue>
			<exportTimeZone>
				<timeZoneOffset>+01:00</timeZoneOffset>
			</exportTimeZone>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>Meteo_Process_Operational_Grids</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>T.forecast</parameterId>
			<qualifierId>GFS</qualifierId>
			<locationSetId>GADM2</locationSetId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<readWriteMode>read complete forecast</readWriteMode>
		</timeSeriesSet>
	</export>
</timeSeriesExportRun>
