<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesImportRun.xsd">
	<import>
		<general>
			<importType>NETCDF-CF_GRID</importType>
			<!--<serverUrl>https://nomads.ncep.noaa.gov:9090/dods/gens/gens%TIME_ZERO(yyyyMMdd)%/gep_all_%RELATIVE_TIME_IN_SECONDS(HH,-21600)%z</serverUrl>-->
			<serverUrl>https://nomads.ncep.noaa.gov/dods/gefs//gefs%TIME_ZERO(yyyyMMdd)%/gefs_pgrb2ap5_all_%TIME_ZERO(HH)%z</serverUrl>
			<idMapId>IdImportGFS</idMapId>
			<unitConversionsId>ImportUnitConversions</unitConversionsId>
			<missingValue>-3.402823E38</missingValue>
			<importTimeZone>
				<timeZoneName>GMT</timeZoneName>
			</importTimeZone>
			<dataFeedId>GEFS.Server</dataFeedId>
			<expiryTime unit="day" multiplier="7"/>
		</general>
		<timeSeriesSet>
			<moduleInstanceId>Import_GEFS_Server</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>P.forecast</parameterId>
			<locationId>GEFS</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="6"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>6</synchLevel>
			<ensembleId>GEFS</ensembleId>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>Import_GEFS_Server</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>T.forecast</parameterId>
			<locationId>GEFS</locationId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="6"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>6</synchLevel>
			<ensembleId>GEFS</ensembleId>
		</timeSeriesSet>
		<externUnit parameterId="P.forecast" unit="mm" cumulativeSum="false"/>
		<externUnit parameterId="T.forecast" unit="K"/>
	</import>
</timeSeriesImportRun>
