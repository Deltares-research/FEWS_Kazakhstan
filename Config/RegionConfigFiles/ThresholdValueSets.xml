	<thresholdValueSet id="P" name="Rainfall">
		<levelThresholdValue>
			<levelThresholdId>Rainfall_Med</levelThresholdId>
			<value>10</value>
		</levelThresholdValue>
		<levelThresholdValue>
			<levelThresholdId>Rainfall_High</levelThresholdId>
			<value>25</value>
		</levelThresholdValue>
		<timeSeriesSet>
			<moduleInstanceId>Meteo_Process_Operational_Grids</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.forecast</parameterId>
			<qualifierId>GFS</qualifierId>
			<locationSetId>meteo_stations</locationSetId>
			<timeSeriesType>external forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
