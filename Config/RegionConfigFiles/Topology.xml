			<localRun>false</localRun>
			<showRunApprovedForecastButton>true</showRunApprovedForecastButton>
		</node>
		<node id="Import_forecasts" name="Import meteo forecasts (GFS)">
			<workflowId>Import_Forecasts</workflowId>
			<graceTime unit="hour" multiplier="24"/>
			<localRun>false</localRun>
			<showRunApprovedForecastButton>true</showRunApprovedForecastButton>
		</node>
		<node id="Import_ensembles" name="Import meteo ensembles (GEFS)">
			<workflowId>Import_Ensemble_Forecasts</workflowId>
