			<showRunApprovedForecastButton>true</showRunApprovedForecastButton>
		</node>
		<node id="Import_earthobservation" name="Import earth observations (GPM Rainfall)">
			<workflowId>Import_EarthObservation</workflowId>
			<graceTime unit="hour" multiplier="6"/>
			<localRun>false</localRun>
			<showRunApprovedForecastButton>true</showRunApprovedForecastButton>
		</node>
		<node id="pocess_meteo_grids" name="Process meteo grids">
