	<nodes id="Import_Op" name="Import and Process Operational Data">
		<node id="Import_hydro_observations" name="Import hydro observations (GRDC)">
			<workflowId>Import_GRDC</workflowId>
			<viewPermission>NotVisible</viewPermission>
